
<div class="panel-group screen">
<div class="panel-heading">
       <h3>Incidentes</h3>
    <button class="btn btn-primary"" ng-click="NuevoIncidenteModal()">
            <i class="fa fa-plus-circle"></i> Nuevo Incidente
        </button> 
</div>
<div class="panel-collapse in">
<div class="panel-body">
    <div class="row col-md-10">
    <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Clientes:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid" 
            data-html="1" data-multiple="1" bs-options="" 
            bs-select>Selecciona un Cliente...&nbsp;<span class="caret"></span></button>
        </div>
    </div>
    <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Productos:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid" 
            data-html="1" data-multiple="1" bs-options="" 
            bs-select>Selecciona un Producto...&nbsp;<span class="caret"></span></button>
        </div>
    </div>

    <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Modulos:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid"  
            data-html="1" data-multiple="1" bs-options="" 
            bs-select="">Selecciona un Modulo...&nbsp;<span class="caret"></span></button>
        </div>
    </div>

    <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Componentes:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid"  
            data-html="1" data-multiple="1" bs-options="" 
            bs-select="">Selecciona un Elemento...&nbsp;<span class="caret"></span></button>
        </div>
    </div>

    <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Lider:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid"  
            data-html="1" data-multiple="1" bs-options="" 
            bs-select="">Selecciona un Lider...&nbsp;<span class="caret"></span></button>
        </div>
    </div>

    <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Consultor:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid"  
            data-html="1" data-multiple="1" bs-options="" 
            bs-select="">Selecciona un Consultor...&nbsp;<span class="caret"></span></button>
        </div>
    </div>


     <div class="col-md-3">
        <div class="form-group">
            <div class="col-md-12">
                <label>Tester:&nbsp;</label>
            </div>
            <button type="button" class="btn btn-default ng-pristine ng-untouched ng-valid"  
            data-html="1" data-multiple="1" bs-options="" 
            bs-select="">Selecciona un Tester...&nbsp;<span class="caret"></span></button>
        </div>
    </div>
 </div>
    <div class="row">
        
  </div>
    <div class="col-md-10">
    <div class="pull-center">
     <div class="checkbox" style="display: inline-block; margin-right: 10px;">
       <label>
        <input type="checkbox" @*ng-model="Ctrl.bMostrarQC"*@ style="height:20px;width: 10%;margin-top: 0px;margin-left:-25px;" class="ng-pristine ng-untouched ng-valid">
         Mostrar Control de Calidad
        </label>
    </div>
    <span class="btn btn-primary" @*ng-click="Ctrl.ObtenerActividadesFiltradas()*@"><i class="fa fa-search"></i> Buscar</span>
    <span class="btn btn-warning" @*ng-click="actividadesPendientes()"*@><i class="fa fa-exclamation-circle"></i> Solo Pendientes</span>
    <button class="btn btn-info" type="submit"><i class="fa fa-file-excel-o"></i> Exportar a Excel</button>
  </div>
 </div>


<div class=" col-md-12">
    <div class="row">
        <md-card>
            <md-table-container>
                <table border="1"  aling="center" md-table ng-model="LstActividades" md-progress="promise">
                    <thead md-head md-order="queryActividad.order">
                    <tr>
                    <th  style="color:black;"> Id </th>
                    <th  style="color:black;"> Incidente</th>
                    <th  style="color:black;"> Cliente</th>
                    <th  style="color:black;"> Producto</th>
                    <th  style="color:black;"> Módulo</th>

                    <th  style="color:black;"> Componente</th>
                    <th  style="color:black;"> T.Solicitado UE</th>
                    <th  style="color:black;"> T.Autorizado</th>
                    <th  style="color:black;"> Tra.Realizado</th>
                    <th  style="color:black;"> Tra.Restante</th>
                    <th  style="color:black;"> Dias trans</th>
                    <th  style="color:black;"> Avance</th>
                    <th  style="color:black;"> Vuelta</th>
                    <th  style="color:black;"> Estatus</th>
                    <th  style="color:black;"> Fecha registro</th>
                    <th  style="color:black;"> #</th>
                    </tr>
                    </thead>
                    <tbody md-body>
                 
                    </tbody>
                </table>
                </md-table-container>
        </md-card>
    </div>
</div>
    </div>
  </div>
</div>
